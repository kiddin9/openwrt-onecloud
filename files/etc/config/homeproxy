
config homeproxy 'infra'
	option __warning 'DO NOT EDIT THIS SECTION, OR YOU ARE ON YOUR OWN!'
	option common_port '22,53,80,143,443,465,853,873,993,995,8080,8443,9418'
	option mixed_port '5330'
	option redirect_port '5331'
	option tproxy_port '5332'
	option dns_port '5333'
	option china_dns_port '5334'
	option tun_name 'singtun0'
	option tun_addr4 '172.19.0.1/30'
	option tun_addr6 'fdfe:dcba:9876::1/126'
	option tun_mtu '9000'
	option table_mark '100'
	option self_mark '100'
	option tproxy_mark '101'
	option tun_mark '102'

config homeproxy 'config'
	option routing_mode 'proxy_mainland_china'
	option routing_port 'all'
	option proxy_mode 'redirect_tproxy'
	option ipv6_support '0'
	option main_node 'nil'
	option main_udp_node 'nil'
	option dns_server '8.8.8.8'

config homeproxy 'control'
	option lan_proxy_mode 'disabled'
	list wan_proxy_ipv4_ips '91.108.4.0/22'
	list wan_proxy_ipv4_ips '91.108.8.0/22'
	list wan_proxy_ipv4_ips '91.108.12.0/22'
	list wan_proxy_ipv4_ips '91.108.56.0/22'
	list wan_proxy_ipv4_ips '95.161.64.0/20'
	list wan_proxy_ipv4_ips '149.154.160.0/22'
	list wan_proxy_ipv4_ips '149.154.164.0/22'
	list wan_proxy_ipv4_ips '149.154.172.0/22'

config homeproxy 'routing'
	option sniff_override '0'
	option default_outbound '手动选择'
	option bypass_cn_traffic '0'

config homeproxy 'dns'
	option dns_strategy 'prefer_ipv4'
	option default_server 'default-dns'
	option default_strategy 'prefer_ipv4'

config homeproxy 'subscription'
	option auto_update '0'
	option allow_insecure '0'
	option packet_encoding 'xudp'
	option update_via_proxy '0'
	option filter_nodes 'disabled'

config homeproxy 'server'
	option enabled '0'
	option auto_firewall '0'

config homeproxy 'experimental'
	option enable_clash_api '1'
	option external_controller '192.168.2.2:9090'
	option external_ui '/etc/homeproxy/ui/'
	option external_ui_download_url 'https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip'
	option external_ui_download_detour '自动选择'
	option default_mode 'rule'

config node 'sptest'
	option label '自动选择'
	option type 'urltest'

config node 'select'
	option label '手动选择'
	option type 'selector'
	list outbounds 'direct-out'
	list outbounds 'block-out'

config node 'GLOBAL'
	option label 'GLOBAL'
	option type 'selector'
	list outbounds '自动选择'
	list outbounds '手动选择'
	option default '手动选择'

config ruleset 'cnip'
	option label 'cnip'
	option enabled '1'
	option type 'remote'
	option format 'binary'
	option url 'https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo-lite/geoip/cn.srs'
	option outbound '自动选择'

config ruleset 'cnsite'
	option label 'cnsite'
	option enabled '1'
	option type 'remote'
	option format 'binary'
	option url 'https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo-lite/geosite/cn.srs'
	option outbound '自动选择'

config dns_rule 'node'
	option label 'node'
	option enabled '1'
	option mode 'default'
	list outbound 'any'
	option server 'default-dns'

config dns_rule 'clashdirect'
	option label 'clashdirect'
	option enabled '1'
	option mode 'default'
	option clash_mode 'direct'
	option server 'default-dns'

config dns_rule 'clashglobal'
	option label 'clashglobal'
	option enabled '1'
	option mode 'default'
	option clash_mode 'global'
	option server 'default-dns'

config dns_rule 'sitecn'
	option label 'sitecn'
	option enabled '1'
	option mode 'default'
	list rule_set 'cnsite'
	option server 'default-dns'

config routing_rule 'direct'
	option label 'direct'
	option enabled '1'
	option mode 'default'
	option source_ip_is_private '0'
	option ip_is_private '0'
	option clash_mode 'direct'
	option rule_set_ipcidr_match_source '0'
	option outbound 'direct-out'

config routing_rule 'global'
	option label 'global'
	option enabled '1'
	option mode 'default'
	option source_ip_is_private '0'
	option ip_is_private '0'
	option clash_mode 'global'
	option rule_set_ipcidr_match_source '0'
	option outbound 'GLOBAL'

config routing_rule 'geocn'
	option label 'geocn'
	option enabled '1'
	option mode 'default'
	option source_ip_is_private '0'
	option ip_is_private '0'
	list rule_set 'cnip'
	list rule_set 'cnsite'
	option rule_set_ipcidr_match_source '0'
	option outbound 'direct-out'

